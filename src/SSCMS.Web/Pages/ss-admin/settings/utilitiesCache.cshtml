@page
@{ Layout = "_Layout"; }

<el-table
  class="headerless"
  :data="parameters"
  style="width: 100%">
  <el-table-column
    width="280">
    <template slot-scope="scope">
      <strong>{{scope.row.key}}</strong>
    </template>
  </el-table-column>
  <el-table-column>
    <template slot-scope="scope">
      {{scope.row.value}}
    </template>
  </el-table-column>
</el-table>

<el-divider></el-divider>
<div style="height: 10px"></div>
<el-button size="small" type="primary" v-on:click="btnCleanClick">清空缓存</el-button>

@section Scripts{ <script src="/sitefiles/assets/js/admin/settings/utilitiesCache.js" type="text/javascript"></script> }